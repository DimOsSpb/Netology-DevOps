[webservers]
%{ for i in webs ~}
${i.name}   ansible_host=${i.network_interface[0].nat_ip_address}    fqdn=${i.fqdn}
%{ endfor ~}

[databases]
%{ for i in dbs ~}
${i.name}   ansible_host=${i.network_interface[0].nat_ip_address}    fqdn=${i["fqdn"]}
%{ endfor ~}

[storages]
%{ for i in storages ~}
${i.name}   ansible_host=${i.network_interface[0].nat_ip_address}    fqdn=${i.fqdn}
%{ endfor ~}